{
  "version": 2,
  "name": "connect.sweetbit.io",
  "alias": "connect.sweetbit.io",
  "regions": ["bru", "sfo", "syd"],
  "scope": "the-lightning-land",
  "env": {
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/index.html"
    }
  ]
}
